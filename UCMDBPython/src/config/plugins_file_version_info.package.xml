            <module>plugin_ntcmd_file_version</module>
            <class>FileVersionInformationPluginByNTCMD</class>
            <qualifiers>
                <qualifier type="protocol">ntadmin</qualifier>
				<qualifier type="protocol">uda</qualifier> 
				<qualifier type="os">Windows</qualifier>
				<qualifier type="application">PostgreSQL</qualifier>
				<qualifier type="application">IBM DB2</qualifier>
				<qualifier type="application">Symantec pcAnywhere</qualifier>
				<qualifier type="application">Symantec pcAnywhere Gateway</qualifier>
				<qualifier type="application">Cisco VPN Client</qualifier>
            </qualifiers>
        </plugin>
        <plugin id="wmi_file_version_information">
            <name>File Version Information by WMI</name>
            <description>Sets file version information by WMI</description>
            <module>plugin_wmi_file_version</module>
            <class>FileVersionInformationPluginByWMI</class>
            <qualifiers>
                <qualifier type="protocol">wmi</qualifier>
				<qualifier type="os">Windows</qualifier>
				<qualifier type="application">PostgreSQL</qualifier>
				<qualifier type="application">IBM DB2</qualifier>
				<qualifier type="application">Symantec pcAnywhere</qualifier>
