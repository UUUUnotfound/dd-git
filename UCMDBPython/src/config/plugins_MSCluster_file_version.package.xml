<?xml version="1.0" encoding="UTF-8"?>
<package parserClassName="com.hp.ucmdb.discovery.library.communication.downloader.cfgfiles.PluginsPackageConfigFile">
    <plugins>
        <plugin id="ntcmd_mscluster_version_information">
            <name>Microsoft Cluster Version Information by NTCMD</name>
            <description>Sets file version information by Windows shell</description>
            <module>plugin_mscluster_ntcmd_version</module>
            <class>MSClusterInformationPluginByNTCMD</class>
            <qualifiers>
                <qualifier type="protocol">ntadmin</qualifier>
				<qualifier type="protocol">uda</qualifier> 
				<qualifier type="os">Windows</qualifier>
                <qualifier type="protocol">powershell</qualifier>
				<qualifier type="application">Microsoft Cluster SW</qualifier>
            </qualifiers>
        </plugin>
        <plugin id="wmi_mscluster_version_information">
            <name>Microsoft Cluster Version Information by WMI</name>
            <description>Sets file version information by WMI</description>
            <module>plugin_mscluster_wmi_version</module>
            <class>MSClusterInformationPluginByWMI</class>
            <qualifiers>
                <qualifier type="protocol">wmi</qualifier>
				<qualifier type="os">Windows</qualifier>
				<qualifier type="application">Microsoft Cluster SW</qualifier>
            </qualifiers>
        </plugin>
    </plugins>
</package>
