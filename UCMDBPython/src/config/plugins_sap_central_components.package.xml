            <dependencies>
                <module>entity</module>
                <module>command</module>
                <module>cmdlineutils</module>
                <module>iteratortools</module>
                <module>db_platform</module>
                <module>db_builder</module>
                <module>db</module>
	            <module>jdbc_url_parser</module>
