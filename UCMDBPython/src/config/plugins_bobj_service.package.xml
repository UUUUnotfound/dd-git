<?xml version="1.0" encoding="UTF-8" ?>
<package
    parserClassName="com.hp.ucmdb.discovery.library.communication.downloader.cfgfiles.PluginsPackageConfigFile">
    <plugins>
        <plugin id="bobj_crystal_reports">
            <name>BO Crystal Reports</name>
            <description>Determine Business Objects Crystal Reports Component</description>
            <module>plugins_bobj_service</module>
            <class>BobjCrystalReportsPlugin</class>
            <qualifiers>
                <qualifier type="application">BO Crystal Reports</qualifier>
                <qualifier type="protocol">ntadmin</qualifier>
                <qualifier type="protocol">powercmd</qualifier>
                <qualifier type="protocol">powershell</qualifier>
                <qualifier type="protocol">telnet</qualifier>
                <qualifier type="protocol">ssh</qualifier>
            </qualifiers>
            <dependencies>
                <module>cmdlineutils</module>
                <module>entity</module>
                <module>fptools</module>
            </dependencies>
        </plugin>
        <plugin id="bobj_explorer">
            <name>BO Explorer</name>
            <description>Determine Business Objects Explorer Component</description>
            <module>plugins_bobj_service</module>
            <class>BobjExplorerPlugin</class>
            <qualifiers>
                <qualifier type="application">BO Explorer</qualifier>
                <qualifier type="protocol">ntadmin</qualifier>
                <qualifier type="protocol">powercmd</qualifier>
                <qualifier type="protocol">powershell</qualifier>
                <qualifier type="protocol">telnet</qualifier>
                <qualifier type="protocol">ssh</qualifier>
            </qualifiers>
            <dependencies>
                <module>cmdlineutils</module>
                <module>entity</module>
                <module>fptools</module>
            </dependencies>
        </plugin>
    </plugins>
</package>